{
	"env": {
		"browser": true,
		"es6": true
	},
	"parser": "babel-eslint",
	"parserOptions": {
		"ecmaVersion": 8,
		"sourceType": "module",
		"ecmaFeatures": {
			"jsx": true,
			"modules": true
		}
	},
	"plugins": ["prettier", "react"],
	"extends": ["prettier", "plugin:react/recommended"],
	"rules": {
		"prettier/prettier": "error"
	},
	"settings": {
		"react": {
			"version": "detetect"
		}
	}
}